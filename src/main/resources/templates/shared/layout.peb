{% import "shared/nav" %}
<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="description" content="Recipe generator based on ingredients. Helps limit food waste, and allows to try new recipes">
    <meta name="keywords" content="Food, Recipe, LLMs">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="apple-touch-icon" sizes="180x180" href="{{ href("/apple-touch-icon.png")}}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ href("/favicon-32x32.png")}}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ href("/favicon-16x16.png")}}">
    <link rel="manifest" href=href="{{ href("/site.webmanifest")}}">
    <link href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="{{ href("/recipe.css") }}">
    <link rel="stylesheet" href="{{ href("/style.css") }}">
    <link rel="stylesheet" href="{{ href("/create.css") }}">
    <link rel="stylesheet" href="{{ href("/login.css") }}">
    <link rel="stylesheet" href="{{ href("/recipeDetail.css") }}">
    <title>{% block title %}Title{% endblock %}</title>
</head>
<body>
    {{ nav(user,request.servletPath) }}
    <main>
                {% block content %}
                {% endblock %}
    </main>
</body>
</html>
